# synopsis {{{
# Show the VIRTUAL_ENV environment variable trimmed to the last path item.
#
# If it is just "venv", use it's parent folder name instead.
# }}}

# don't let venv change ps1
export VIRTUAL_ENV_DISABLE_PROMPT=1

zincs_virtualenv ()
{
  if [[ "${VIRTUAL_ENV:t}" == "venv" ]]; then
    REPLY=${VIRTUAL_ENV:h:t}
  else
    REPLY="${VIRTUAL_ENV:t}"
  fi
}

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

zincs_virtualenv_default_opts ()
{
  reply=(black blue CONDITIONAL normal)
}

# AUTOGENERATED - DO NOT EDIT

# AUTOGENERATED - DO NOT EDIT
# vim: ft=zsh ts=2 sw=2 et fenc=utf-8

function zincs_virtualenv_display_hidden () {
  [[ -z "${VIRTUAL_ENV}" ]] && true || false
}

